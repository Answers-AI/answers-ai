{
  "name": "answers-ai",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "dotenv -- turbo run build",
    "db:migrate": "dotenv -- pnpm --filter web db:migrate",
    "db:deploy": "dotenv -- turbo run db:deploy",
    "start": "dotenv -- turbo run start dev-docker dev-inngest",
    "dev": "dotenv -- turbo run dev dev-docker dev-inngest",
    "dev-docker": "docker-compose up -d",
    "dev-inngest": "npx inngest-cli@latest dev -u http://localhost:3000/api/inngest",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "dotenv-cli": "latest",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "prisma": "^4.10.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.12.2"
}
